#include<stdio.h>
#include<iostream>
#include<string.h>
using namespace std;

//第一题 杨辉三角
class A {
public:
	void print(void);
};

void A::print(void) {
	const int N = 15;
	int a[N][N];
	int n, i, j;
	cin >> n;
	for (i = 1; i <= n; i++){
		for (j = 1; j <= i; j++){
			if (i == j || j == 1)
				a[i][j] = 1;
			else
				a[i][j] = a[i - 1][j] + a[i - 1][j - 1];
				cout << " ";
				cout << a[i][j];
		}
		cout << endl;
	}
}

int main() {
	int N;
	A a1;
	cin >> N;
	for (int i = 0; i < N; i++) {
		a1.print();
	}
}

//第二题 进制转换
class A {
public:
	int n;
	void transf(int n);
};

//进制转换函数
 void A::transf(int n) {
	int bit[5] = {0};
	int i = 0;
	while (n > 0) {
		bit[i] = n % 8;
		n = n / 8;
		i++;
	}
	for (int k = 4; k >=0; k--) {
		cout << bit[k];
	}
}

int main() {
	A a1;
	cin >> a1.n;
	a1.transf(a1.n);
}

//第三题 今天星期几
class A {
public:
	int y, m, d;
	void calcu(int y,int m ,int d);
};

void A::calcu(int y,int m, int d) {
	int w;
	if (m == 1)m = 13; if (m == 2)m = 14;
	w =( y % 100 + (y % 100 / 4) + (y/100 / 4) - 2*(y/100) +
		(26*(m + 1) / 10) + d - 1)%7;
	if (w == 0)cout << "Sun";
	else if (w == 1)cout << "Mon";
	else if (w == 2)cout << "Tue";
	else if (w == 3)cout << "Wed";
	else if (w == 4)cout << "Thu";
	else if (w == 5)cout << "Fri";
	else if (w == 6)cout << "Sat";
}

int main() {
	A a1;
	scanf_s("%d-%d-%d", &a1.y, &a1.m, &a1.d);
	a1.calcu(a1.y, a1.m, a1.d);
}
