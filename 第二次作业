#include<stdio.h>
#include<iostream>
#include<string.h>
using namespace std;

//第一题 杨辉三角
class A {
public:
	void print(void);
};

void A::print(void) {
	const int N = 15;
	int a[N][N];
	int n, i, j;
	cin >> n;
	for (i = 1; i <= n; i++){
		for (j = 1; j <= i; j++){
			if (i == j || j == 1)
				a[i][j] = 1;
			else
				a[i][j] = a[i - 1][j] + a[i - 1][j - 1];
				cout << " ";
				cout << a[i][j];
		}
		cout << endl;
	}
}

int main() {
	int N;
	A a1;
	cin >> N;
	for (int i = 0; i < N; i++) {
		a1.print();
	}
}

//第二题 进制转换
class A {
public:
	int n;
	void transf(int n);
};

//进制转换函数
 void A::transf(int n) {
	int bit[5] = {0};
	int i = 0;
	while (n > 0) {
		bit[i] = n % 8;
		n = n / 8;
		i++;
	}
	for (int k = 4; k >=0; k--) {
		cout << bit[k];
	}
}

int main() {
	A a1;
	cin >> a1.n;
	a1.transf(a1.n);
}
